<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title> Transaction Page</title>
  <style>
     body {
      margin: 0;
      padding: 0;
      background-color: #121212;
      font-family: 'Segoe UI', sans-serif;
      background: linear-gradient(135deg, #ff6ec4, #7873f5);
      color: #f0f0f0;
    }

  

      table {
    border-collapse: separate; /* Important: Allows spacing */
    border-spacing: 10px; /* Space between rows and columns */
    width: 100%;
  }

  th{
    border: 1px solid #ccc;
    padding: 10px;
    text-align: left;
   
  }

    td {
    border: 1px solid #ccc;
    padding: 10px;
    text-align: left;
   
  }

 

  </style>
</head>
<body>

<center><h1>Transaction List</h1></center>
 
<table border="1">
  <tr>
    <th>Date</th>
    <th>Time</th>
    <th>Category</th>
    <th>Description</th>
    <th>Amount</th>

  </tr>

  <% data.forEach(function(item) { %>
    <tr>
      <td><%= item.date %></td>
      <td><%= item.time %></td>
      <td><%= item.category %></td>
      <td><%= item.description %></td>
       <td><%= item.amount %></td>
        
    </tr>
  <% }); %>
</table>

   

</body>
</html>
