<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title> Transaction Page</title>
  <style>
     body {
      margin: 0;
      padding: 0;
      background-color: #121212;
      font-family: 'Segoe UI', sans-serif;
      background: linear-gradient(135deg, #ff6ec4, #7873f5);
      color: #f0f0f0;
    }

  </style>
</head>
<body>

 <h1>Transaction List</h1>
<table border="1">
  <tr>
    <th>Date</th>
    <th>Category</th>
    <th>Description</th>
    <th>Amount</th>
  </tr>

  <% data.forEach(function(item) { %>
    <tr>
      <td><%= item.date %></td>
      <td><%= item.category %></td>
      <td><%= item.description %></td>
       <td><%= item.amount %></td>
        
    </tr>
  <% }); %>
</table>




